    "# read in the list of tickers from example csv file\n",
    "listed_stocks = pd.read_csv('Tickers_Example.csv')\n",
    "listed_stocks.head()\n",
    "\n",
    "# Benchmark Average: Arithmetic Average of the total return of the TSX Composite and S&P 500 over the contest period\n",
    "def get_total_return(ticker,time):\n",
    "    shares = yf.Ticker(ticker).history(period=time)['Close']\n",
    "    shares = shares.dropna()\n",
    "\n",
    "    # This if statment ensures we have at least 2 trading days\n",
    "    if len(shares) < 2:\n",
    "        raise ValueError(\"Not enough data returned for \" + ticker)\n",
    "    \n",
    "    start = shares.iloc[0]\n",
    "    end = shares.iloc[-1]\n",
    "\n",
    "    return (end-start)/start\n",
    "\n",
    "sp_return = get_total_return('^GSPC', '7d')\n",
    "tsx_return = get_total_return('^GSPTSE', '7d')\n",
    "benchmark_average = (sp_return + tsx_return) / 2\n",
    "print(\"SP500 Return:\", sp_return*100, \"%\")\n",
    "print(\"TSX Return:\", tsx_return*100, \"%\")\n",
    "print(\"Benchmark Average:\", benchmark_average*100, \"%\")"
   ]
